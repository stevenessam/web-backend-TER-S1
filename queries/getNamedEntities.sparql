  PREFIX dct:    <http://purl.org/dc/terms/>
  PREFIX oa:     <http://www.w3.org/ns/oa#>
  PREFIX rdfs:   <http://www.w3.org/2000/01/rdf-schema#>
  PREFIX schema: <http://schema.org/>
  PREFIX skos:   <http://www.w3.org/2004/02/skos/core#>
  PREFIX skosxl: <http://www.w3.org/2008/05/skos-xl#>
  PREFIX oa: <http://www.w3.org/ns/oa#>
  prefix bibo:    <http://purl.org/ontology/bibo/>
  prefix d2kab:   <http://ns.inria.fr/d2kab/>
  prefix dct:     <http://purl.org/dc/terms/>
  prefix fabio:   <http://purl.org/spar/fabio/>
  prefix frbr:    <http://purl.org/vocab/frbr/core#>
  prefix ncbi:    <http://identifiers.org/taxonomy/>
  prefix oa:      <http://www.w3.org/ns/oa#>
  prefix owl:     <http://www.w3.org/2002/07/owl#>
  prefix skos:    <http://www.w3.org/2004/02/skos/core#>
  prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
  prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#>
  prefix xsd:     <http://www.w3.org/2001/XMLSchema#>

  SELECT DISTINCT ?entityText ?startPos ?endPos ?entityUri ?entityLabel
  FROM <http://ns.inria.fr/d2kab/graph/wheatgenomicsslkg>
  WHERE {
      <{id}> a fabio:ResearchPaper;
        dct:abstract ?abstract.
      ?subject frbr:partOf ?abstract.
    
    [] a oa:Annotation;
        oa:hasTarget [ oa:hasSource ?subject ;
                      oa:hasSelector [ oa:exact ?entityText;
                                        oa:start ?startPos;
                                        oa:end ?endPos ]] ;
        oa:hasBody ?entityUri.

      OPTIONAL {
          ?entityUri rdfs:label|skos:prefLabel|dct:title|schema:url ?entityLabel.
      }
  }
